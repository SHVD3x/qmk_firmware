    "manufacturer": "MonsGeek",
    "maintainer": "jonylee@hfd",
    "usb": {
        "force_nkro": true,
        "vid": "0xFFFE",
        "pid": "0x0005",
        "device_version": "1.1.0",
        "suspend_wakeup_delay": 400 // ?????
    },
    "processor": "WB32FQ95",
    "bootloader": "wb32-dfu",
    "features": {
        "bootmagic": true,
        "mousekey": false,
        "extrakey": true,
        "console": false,
        "command": false,
        "nkro": true,
        "encoder": true,
        "rgb_matrix": true
    },
    "build": {
        "debounce_type": "sym_defer_pk",
        "lto": true
    },
    /*"matrix_pins": {
        "cols": ["C1", "C2", "C3", "A0", "A1", "A2", "A3", "A4", "A5", "A6", "A7", "C4", "C5", "B0", "B1", "B2"],
        "rows": ["B15", "C6", "C7", "C8", "C9", "A8"]
    },
    "diode_direction": "ROW2COL",
    */
    // После изменения направления диодов на COL2ROW нужно всю раскладку переписывать по аналогии с YMD09
    // Раскладка пока еще не переписывалась
    "matrix_pins": {
        "cols": ["B15", "C6", "C7", "C8", "C9", "A8"],
        "rows": ["C1", "C2", "C3", "A0", "A1", "A2", "A3", "A4", "A5", "A6", "A7", "C4", "C5", "B0", "B2"] // For default scanning matrix add B1 to the end and change code below accordingly.
    }, // Pin B1 is present in matix for only two buttons and it was removed here for optimization. Since pin for 14 coord was removed, 15 need to be changed to 14 now.
    "diode_direction": "COL2ROW",
    "encoder": {
        "rotary": [
            {"pin_a": "B14", "pin_b": "B13"}
        ]
    },
    "qmk": {
        "tap_keycode_delay": 5,
    },
    "rgb_matrix": {
        "driver": "IS31FL3733",
        "max_brightness": 200, // Можно попробовать максимум
        "val_steps": 20,
        "animations": { // Подкорректировать эффекты
